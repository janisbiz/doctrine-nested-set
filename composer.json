{
  "name": "janisbiz/doctrine-nested-set",
  "description": "Doctrine extension to support nested set model",
  "keywords": [
    "behaviors",
    "doctrine2",
    "extensions",
    "janisbiz",
    "tree",
    "nestedset"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Janis Bizkovskis"
    }
  ],
  "require": {
    "php": ">=7.1",
    "behat/transliterator": "~1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "squizlabs/php_codesniffer": "^3.1",
    "wimg/php-compatibility": "^8.0",
    "doctrine/orm": "^2.6",
    "doctrine/common": "^2.9",
    "behat/behat": "^3.5",
    "symfony/yaml": "^4.1",
    "symfony/var-dumper": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Janisbiz\\DoctrineNestedSet\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": "\"./vendor/bin/phpcs\" --config-set installed_paths vendor/wimg/php-compatibility",
    "post-update-cmd" : "\"./vendor/bin/phpcs\" --config-set installed_paths vendor/wimg/php-compatibility"
  }
}
